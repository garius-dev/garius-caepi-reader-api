{
  "UrlSettings": {
    "FrontendBaseUrl": "https://localhost:7090/api/v1/auth",
    "EmailConfirmationPath": "/confirm-email",
    "TenantEmailConfirmationPath": "/confirm-signup",
    "TenantInviteEmailConfirmationPath": "/confirm-invite",
    "PasswordResetPath": "/reset-password",
    "InvitationPath": "/register-from-invite",
    "ValidateInvitationPath": "/intivations/validate"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GoogleSecretManager": {
    "ProjectId": "garius-tcm"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "https://localhost:7223"
    ]
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*"
}
